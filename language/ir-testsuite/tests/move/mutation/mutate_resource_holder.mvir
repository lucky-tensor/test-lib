module A {
import 0x1.Coin1;
    import 0x1.Diem;
    resource A { c: Diem.Diem<Coin1.Coin1> }
    public new(c: Diem.Diem<Coin1.Coin1>): Self.A {
        return A { c: move(c) };
    }
    public destroy_a(a: Self.A) {
        let c: Diem.Diem<Coin1.Coin1>;
        A { c } = move(a);
        Diem.destroy_zero<Coin1.Coin1>(move(c));
        return;
    }
    public mutate(a_ref: &mut Self.A) {
        let ref: &mut Diem.Diem<Coin1.Coin1>;
        let zero: Diem.Diem<Coin1.Coin1>;

        ref = &mut move(a_ref).c;
        zero = Diem.zero<Coin1.Coin1>();
        *move(ref) = move(zero);

        return;
    }
}

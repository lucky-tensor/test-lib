module RTest {
import 0x1.Coin1;
    import 0x1.Diem;
    resource T{fr: Diem.Diem<Coin1.Coin1>}

    public new(y: Diem.Diem<Coin1.Coin1>): Self.T {
        return T{fr: move(y)};
    }

    public destroy(t: Self.T){
        let fr: Diem.Diem<Coin1.Coin1>;
        T { fr } = move(t);
        Diem.destroy_zero<Coin1.Coin1>(move(fr));
        return;
    }

    public t4(t: &mut Self.T, i: Diem.Diem<Coin1.Coin1>) {
        let x: &mut Diem.Diem<Coin1.Coin1>;
        x = &mut move(t).fr;
        *move(x) = move(i);
        return;
    }
}

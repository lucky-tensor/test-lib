//! new-transaction
import 0x1.DiemAccount;
import 0x1.Signer;

main(account: &signer) {
    let sender: address;
    sender = Signer.address_of(move(account));
    assert(DiemAccount.sequence_number(move(sender)) == 1, 42);
    return;
}

// check: "Keep(EXECUTED)"
